{
    "name": "eborio/laravel-gettext",
    "description": "Adds localization support to laravel applications in an easy way using Poedit and GNU gettext.",
    "homepage": "https://github.com/eborio/laravel-gettext",
    "keywords": [
        "gettext",
        "localization",
        "poedit",
        "laravel-gettext",
        "laravel", "translation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolás Daniel Palumbo",
            "email": "n@xinax.net"
        },
        {
            "name": "Antoine Aflalo",
            "email": "dev+laravel_gettext@zerospam.ca"
        },
        {
            "name": "Eborio Linárez",
            "email": "eborio.linarez@dotworkers.com"
        }
    ],
    "support": {
        "issues": "https://github.com/eborio/laravel-gettext/issues"
    },
    "require": {
        "php": "8.*",
        "laravel/framework": ">=9.0"
    },
    "autoload": {
        "psr-0": {
            "Xinax\\LaravelGettext\\": "src/"
        },
        "files": [
            "src/Xinax/LaravelGettext/Support/helpers.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
          "providers": [
            "Xinax\\LaravelGettext\\LaravelGettextServiceProvider"
          ]
       }
    }
}
